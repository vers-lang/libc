/* stdio.h */
decextern printf
decextern scanf

/* unistd.h */
decextern sleep

decfun c_print
decfun c_scanf
decfun c_sleep

fun c_print() {
    printf();
}

fun c_scanf() {
    scanf();
}

fun c_sleep() {
    sleep();
}